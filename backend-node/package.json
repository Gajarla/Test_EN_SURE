{
    "name": "testassure-backend",
    "version": "1.0.0",
    "description": "Backend",
    "main": "index.js",
    "scripts": {
        "format:check": "prettier --check .",
        "format:write": "prettier --write .",
        "lint:check": "eslint .",
        "lint:fix": "eslint --fix .",
        "start:local": "node -r dotenv/config index.js dotenv_config_path=./environments/local.env",
        "start:dev": "node -r dotenv/config index.js dotenv_config_path=./environments/dev.env",
        "insertDbData": "node -r dotenv/config ./insertDBData/insertData.js dotenv_config_path=./insertDBData/.env",
        "forever": "forever -c \"nodemon --exitcrash\" index.js",
        "server": "set NODE_OPTIONS=--openssl-legacy-provider && nodemon server",
        "client": "npm start --prefix client",
        "updateCompanyOrProject": "node ./utils/updateCompanyOrProjectJob"
    },
    "nodemonConfig": {
        "verbose": true,
        "ignore": [
            "uploads/*",
            "tmp/*"
        ]
    },
    "proxy": "http://localhost:5000",
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.550.0",
        "@aws-sdk/lib-storage": "^3.540.0",
        "@aws-sdk/s3-request-presigner": "^3.758.0",
        "@descope/node-sdk": "^1.6.8",
        "@marsaud/smb2": "^0.18.0",
        "aws-sdk": "^2.1591.0",
        "axios": "^1.6.8",
        "bcryptjs": "^2.4.3",
        "cloudinary": "^1.26.3",
        "concurrently": "^6.2.0",
        "config": "^3.3.6",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "exceljs": "^4.4.0",
        "express": "^4.17.1",
        "express-fileupload": "^1.5.2",
        "form-data": "^4.0.0",
        "googleapis": "^84.0.0",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.0",
        "moment": "^2.29.4",
        "moment-timezone": "^0.5.45",
        "mongodb": "^6.17.0",
        "mongoose": "^8.15.2",
        "multer": "^2.0.1",
        "node-fetch": "^2.6.6",
        "nodemailer": "^6.6.3",
        "nodemon": "^2.0.7",
        "puppeteer": "^23.0.2",
        "query-string": "^7.1.1",
        "quickchart-js": "^3.1.3",
        "request": "^2.88.2",
        "smb2": "^0.2.11",
        "testassure-backend": "file:",
        "winston": "^3.8.2",
        "ws": "^8.6.0",
        "xlsx": "^0.17.4",
        "xml2js": "^0.6.2",
        "xmlbuilder2": "^3.1.1"
    },
    "devDependencies": {
        "eslint": "^8.5.7",
        "eslint-config-prettier": "^8.10.0",
        "prettier": "^3.2.5"
    }
}
